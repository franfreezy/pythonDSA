
#Input: nums = [2,7,11,15], target = 9
#Output: [0,1]
class Solution:
    def twoSum(self, nums: list[int], target: int) -> list[int]:
        for num in nums:
            if num >target:
                return
            if num< target:
                difference=target-num
                if difference in nums:
                    print(nums.index(difference))
                    
                    
                

item=Solution()
nums =[3,3]
#nums = [2,7,11,15]
item.twoSum(nums,6)